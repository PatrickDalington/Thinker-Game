<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cwp.game">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/Theme.Game">
        <activity
            android:name=".launcher.Badges"
            android:exported="false" />
        <activity
            android:name=".launcher.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".alphabets_game_activity._8"
            android:exported="false" />
        <activity
            android:name=".launcher.StarterActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".launcher.Settings"
            android:exported="false" />
        <activity
            android:name=".alphabets_game_activity.AlphabetPuzzleActivity"
            android:exported="false" />
        <activity
            android:name=".launcher.MainActivity"
            android:exported="false"
            android:label="@string/activity_title_launcher" />
        <activity
            android:name=".word_game_activity.DraggableStaggeredWordsActivity"
            android:label="@string/activity_title_demo_d_basic" />
        <activity
            android:name=".alphabets_game_activity.DraggableAlphabetsActivity"
            android:label="@string/activity_title_alphabet" />
        <activity
            android:name=".emoji_game_activity.DraggableEmojiActivity"
            android:label="@string/activity_title_emoji" />
        <activity
            android:name=".numbers_game_activity.DraggableNumbersActivity"
            android:label="@string/activity_title_number" />
    </application>

</manifest>